@model  Estream.Cart42.Web.Domain.ContentPage
@{
    ViewBag.Title = Model.Title;
    ViewBag.NoSidebar = true;
}

<div class="container">
    <div class="row">
        <section class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div id="content">
                <h1>@Model.Title</h1>
                <div class="wrapper">
                    @Html.Raw(Model.Content)
                </div>
            </div>
        </section>
    </div>
</div>